{
  "name": "ai-mental-wellbeing-agent-team",
  "version": "1.0.0",
  "description": "AI Mental Wellbeing Agent Team - TypeScript version",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm src/index.ts",
    "start": "node dist/index.js",
    "bot": "node telegram-bot.js",
    "bot:dev": "nodemon telegram-bot.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:rag": "jest --testPathPattern=rag",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist logs/*.log",
    "type-check": "tsc --noEmit",
    "optimize": "npm run clean && npm run build && npm run lint:fix",
    "migrate:up": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm scripts/migrate-database.ts up",
    "migrate:down": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm scripts/migrate-database.ts down",
    "migrate:reset": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm scripts/migrate-database.ts reset",
    "migrate:seed": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm scripts/migrate-database.ts seed",
    "db:setup": "npm run migrate:up && npm run migrate:seed"
  },
  "keywords": [
    "ai",
    "mental-health",
    "autogen",
    "typescript",
    "agents"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.51.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.28.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^9.31.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
